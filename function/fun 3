/******************************************************************************
This a simple c function program. this  is a munltiple function call
*******************************************************************************/
#include <stdio.h>

int main()
{
    printf("indivisual restaurants categoregies name \n");
    restaurants();
    restaurants_1();
    restaurants_2();

    return 0;
}
int restaurants()
{
    printf("Adarsh Restaurants\n");
}
int restaurants_1()
{
printf("Rahul Pal Best Restaurants\n");
}
int restaurants_2()
{
printf("Rukayansh Restaurants");
}
